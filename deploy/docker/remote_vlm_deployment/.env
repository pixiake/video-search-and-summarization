GRAPH_DB_PASSWORD=password123

MILVUS_DB_HOST=milvus-standalone
MILVUS_DB_GRPC_PORT=19530
MILVUS_DB_HTTP_PORT=9091

ARANGO_DB_HOST=arango-db
