# ============ VLM 配置（阿里云 Qwen3-VL-Plus）============
VLM_MODEL_TO_USE=openai-compat
VIA_VLM_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1
VIA_VLM_API_KEY=你的阿里云API密钥
VIA_VLM_OPENAI_MODEL_DEPLOYMENT_NAME=qwen-vl-plus

# ============ LLM 配置（你自己的服务）============
LLM_MODEL_NAME=qwen2.5-72b-instruct
LLM_BASE_URL=http://你的LLM服务地址/v1
LLM_API_KEY=你的LLM_API密钥或留空

# ============ Embedding 配置（你自己的服务）============
EMBEDDING_MODEL_NAME=bge-m3
EMBEDDING_BASE_URL=http://你的Embedding服务地址/v1
EMBEDDING_API_KEY=你的Embedding_API密钥或留空

# ============ Reranker 配置（可选，如果没有就用embedding的）============
RERANKER_MODEL_NAME=bge-reranker-v2-m3
RERANKER_BASE_URL=http://你的Reranker服务地址/v1
RERANKER_API_KEY=你的Reranker_API密钥或留空

# ============ 数据库配置 ============
GRAPH_DB_HOST=graph-db
GRAPH_DB_BOLT_PORT=7687
GRAPH_DB_HTTP_PORT=7474
